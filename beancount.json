{
    "default_currency": "CNY",
    "ledger_folder": ".",
    "default_ledger": "./example.beancount",
    "cache_path": "beancount.temp",
    "title_format": "{date} * \"{payee}\" \"{comment}\" {tags}",
    "body_format": "  {account:50} {flow:>13.2f} {currency}",
    "regexes": {
        "open": "\\d{4}-\\d{2}-\\d{2} open ([\\w:-]+)",
        "close": "\\d{4}-\\d{2}-\\d{2} close ([\\w:-]+)",
        "payee": "\\d{4}-\\d{2}-\\d{2} \\* \"(.+?)\"[ \\|]+\".*?\"",
        "from": "\n *([\\w:-]+) +-\\d",
        "to": "\n *([\\w:-]+) +\\d",
        "clear": "[^;](\\d{4}-\\d{2}-\\d{2}) ! ?(.*)\\n(.+)\\n(.+)"
    },
    "icons": {
        "Assets": "./icons/Assets.png",
        "Liabilities": "./icons/Liabilities.png",
        "Equity": "./icons/Equity.png",
        "Income": "./icons/Income.png",
        "Expenses": "./icons/Expenses.png"
    }
}